---
home: "/home/swifty"
db: "swygate:DBPASS@127.0.0.1:27017"
keystone:
        address: "127.0.0.1:5000"
        domain: "swifty"
daemon:
        address: "{{ hostvars[groups['swy-core'][0]]['core_private_ip'] }}:8686"
        loglevel: "debug"
        prometheus: "127.0.0.1:9012"
middleware:
        mwseckey: "MWSECPASS"
        maria:
                creds: "root:-@127.0.0.1:3306"
                quotdb: "swifty.QUOTAS"
        rabbit:
                creds: "root:RABBITPASS@127.0.0.1:5672"
                admport: "15672"
        mongo:
                creds: "root:MONGOPASS@127.0.0.1:27017"
        postgres:
                creds: "-:PGRTOKEN@swft-mw:5432"
                admport: "3872"
        s3:
                creds: "-:S3TOKEN@{{ hostvars[groups['swy-core'][0]]['core_private_ip'] }}:8789"
                admport: "{{ hostvars[groups['swy-core'][0]]['core_private_ip'] }}:8787"
                notify: "swifty:S3IFYPASS@127.0.0.1:5672/s3"
                hidden-key-timeout: 120
runtime:
        timeout:
                max: 60
                def: 1
        memory:
                max: 1024
                min: 64
                def: 128
        max-replicas: 32
wdog:
        volume: "/home/swifty-volume"
        port: 8687
logs-keep: 1
